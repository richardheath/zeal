{
    "name": "multiOS",
    "version": "1.0.0",
    "split": true,
    "override": {
        "linux": {
            "files": ["bin/multiOS"],
            "destination": "/temp",
            "scripts": {
                "start": "call bash"
            }
        },
        "windows": {
            "files": ["bin/multiOS.exe"],
            "destination": "c:\\temp",
            "scripts": {
                "start": "call powershell"
            }
        }
    }
}